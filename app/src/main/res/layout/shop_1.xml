<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"  >
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
        android:id="@+id/imgTitle"
        android:layout_width="match_parent"
        android:layout_height="100sp"
        android:scaleType="fitXY"
        android:layout_gravity="center"
            android:src="@drawable/uploadtitle"
            android:onClick="onClick" />

        <ImageButton
            android:layout_width="25sp"
            android:layout_height="25sp"
            android:id="@+id/btnback"
            android:src="@drawable/back2"
            android:background="@android:color/transparent"
            android:scaleType="fitCenter"
            android:layout_marginLeft="10sp"
            android:onClick="onClick"
            android:layout_alignParentBottom="false"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="10sp"
            android:layout_marginBottom="3sp" />
    </RelativeLayout>

    <com.lytech.rainsilk.webviewapp.MyScrollView
        android:id="@+id/scrollView1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >





            <!--<include-->
                <!--android:id="@+id/buy"-->
                <!--layout="@layout/buy_check" />-->

            <TableLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">
                <TableRow>
                    <ImageView
                        android:layout_width="180sp"
                        android:layout_height="100sp"
                        android:id="@+id/imv1"
                        android:src="@drawable/uploadcommodity"
                        android:layout_weight="1"
                        android:onClick="onClick"
                        android:scaleType="fitXY" />

                    <ImageView
                        android:layout_width="180sp"
                        android:layout_height="100sp"
                        android:id="@+id/imv2"
                        android:src="@drawable/uploadcommodity"
                        android:layout_weight="1"
                        android:onClick="onClick"
                        android:scaleType="fitXY" />

                </TableRow>
                <TableRow>
                    <EditText
                        android:id="@+id/tv1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="招牌排骨"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />
                    <EditText
                        android:id="@+id/tv2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="特級排骨2"
                        android:layout_span="2"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />
                </TableRow>
                <TableRow>
                    <EditText
                        android:id="@+id/tvcost1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="140元"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />
                    <EditText
                        android:id="@+id/tvcost2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="100元"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />

                </TableRow>
                <TableRow>
                    <EditText
                        android:id="@+id/tvcount1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="20點"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />
                    <EditText
                        android:id="@+id/tvcount2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="20點"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />

                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">
                <TableRow>
                    <ImageView
                        android:layout_width="180sp"
                        android:layout_height="100sp"
                        android:id="@+id/imv3"
                        android:src="@drawable/uploadcommodity"
                        android:layout_weight="1"
                        android:onClick="onClick"
                        android:scaleType="fitXY" />

                    <ImageView
                        android:layout_width="180sp"
                        android:layout_height="100sp"
                        android:id="@+id/imv4"
                        android:layout_weight="1"
                        android:onClick="onClick"
                        android:scaleType="fitXY"
                        android:src="@drawable/uploadcommodity" />

                </TableRow>
                <TableRow>
                    <EditText
                        android:id="@+id/tv3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="招牌排骨3"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />
                    <EditText
                        android:id="@+id/tv4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="特級排骨4"
                        android:layout_span="2"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />
                </TableRow>
                <TableRow>
                    <EditText
                        android:id="@+id/tvcost3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="90元"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />
                    <EditText
                        android:id="@+id/tvcost4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="80元"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />

                </TableRow>
                <TableRow>
                    <EditText
                        android:id="@+id/tvcount3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="20點"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />
                    <EditText
                        android:id="@+id/tvcount4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="20點"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />

                </TableRow>
            </TableLayout>

            <TableLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">
                <TableRow>
                    <ImageView
                        android:layout_width="180sp"
                        android:layout_height="100sp"
                        android:id="@+id/imv5"
                        android:src="@drawable/uploadcommodity"
                        android:layout_weight="1"
                        android:onClick="onClick"
                        android:scaleType="fitXY" />

                    <ImageView
                        android:layout_width="180sp"
                        android:layout_height="100sp"
                        android:id="@+id/imv6"
                        android:src="@drawable/uploadcommodity"
                        android:layout_weight="1"
                        android:onClick="onClick"
                        android:scaleType="fitXY" />

                </TableRow>
                <TableRow>
                    <EditText
                        android:id="@+id/tv5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="招牌排骨5"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />
                    <EditText
                        android:id="@+id/tv6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="特級排骨6"
                        android:layout_span="2"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />
                </TableRow>
                <TableRow>
                    <EditText
                        android:id="@+id/tvcost5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="70元"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />
                    <EditText
                        android:id="@+id/tvcost6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="65元"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />

                </TableRow>
                <TableRow>
                    <EditText
                        android:id="@+id/tvcount5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="20點"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />
                    <EditText
                        android:id="@+id/tvcount6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="20點"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal" />

                </TableRow>
            </TableLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="請點選圖片進行商家標題及商品圖片上傳"
                android:id="@+id/textView22"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="按上傳鍵確認請勿移開畫面"
                android:id="@+id/textView25"
                android:layout_gravity="center_horizontal" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="上傳"
                android:id="@+id/BUpbutton"
                android:onClick="onClick"
                android:layout_gravity="center_horizontal" />


        </LinearLayout>
    </com.lytech.rainsilk.webviewapp.MyScrollView>

</LinearLayout>
